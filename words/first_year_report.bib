@Article{Li2015-xx,
  author =      {Li, Jin and J\o{}rgensen, Silje F and Maggadottir, S Melkorka and Bakay, Marina and Warnatz, Klaus and Glessner, Joseph and Pandey, Rahul and Salzer, Ulrich and Schmidt, Reinhold E and Perez, Elena and Resnick, Elena and Goldacker, Sigune and Buchta, Mary and Witte, Torsten and Padyukov, Leonid and Videm, Vibeke and Folseraas, Trine and Atschekzei, Faranaz and Elder, James T and Nair, Rajan P and Winkelmann, Juliane and Gieger, Christian and N{\"{o}}then, Markus M and B{\"{u}}ning, Carsten and Brand, Stephan and Sullivan, Kathleen E and Orange, Jordan S and Fevang, B\o{}rre and Schreiber, Stefan and Lieb, Wolfgang and Aukrust, P\aa{}l and Chapel, Helen and Cunningham-Rundles, Charlotte and Franke, Andre and Karlsen, Tom H and Grimbacher, Bodo and Hakonarson, Hakon and Hammarstr{\"{o}}m, Lennart and Ellinghaus, Eva},
  title =       {Association of {CLEC16A} with human common variable immunodeficiency disorder and role in murine {B} cells},
  journal =     {Nat. Commun.},
  year =        {2015},
  volume =      {6},
  pages =       {6804},
  month =       {20~} # apr,
  abstract =    {Common variable immunodeficiency disorder (CVID) is the most
                 common symptomatic primary immunodeficiency in adults,
                 characterized by B-cell abnormalities and inadequate antibody
                 response. CVID patients have considerable autoimmune
                 comorbidity and we therefore hypothesized that genetic
                 susceptibility to CVID may overlap with autoimmune disorders.
                 Here, in the largest genetic study performed in CVID to date,
                 we compare 778 CVID cases with 10,999 controls across 123,127
                 single-nucleotide polymorphisms (SNPs) on the Immunochip. We
                 identify the first non-HLA genome-wide significant risk locus
                 at CLEC16A (rs17806056, P=2.0 \texttimes{} 10(-9)) and confirm
                 the previously reported human leukocyte antigen (HLA)
                 associations on chromosome 6p21 (rs1049225, P=4.8 \texttimes{}
                 10(-16)). Clec16a knockdown (KD) mice showed reduced number of
                 B cells and elevated IgM levels compared with controls,
                 suggesting that CLEC16A may be involved in immune regulatory
                 pathways of relevance to CVID. In conclusion, the CLEC16A
                 associations in CVID represent the first robust evidence of
                 non-HLA associations in this immunodeficiency condition.},
