 University of Michigan, Ann Arbor, MI 48109 and Department of Genetics, University of North Carolina, Chapel Hill, NC 27599, USA Department of Computational Medicine and Bioinformatics, University of Michigan, Ann Arbor, MI 48109, Department of Internal Medicine, Division of Cardiovascular Medicine, University of Michigan, Ann Arbor, MI 48109 and Department of Genetics, University of North Carolina, Chapel Hill, NC 27599, USA.}
}

@Article{SchofieldCarverAchuthanEtAl2016,
  author =      {Schofield, E. C. and Carver, T. and Achuthan, P. and Freire-Pritchett, P. and Spivakov, M. and Todd, J. A. and Burren, O. S.},
